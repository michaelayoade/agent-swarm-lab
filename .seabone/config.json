{
  "max_concurrent_agents": 2,
  "max_retries": 2,
  "agent_timeout_minutes": 30,
  "worktree_base": "agent",
  "auto_review": true,
  "auto_cleanup_days": 3,
  "quality_gates": [
    "shellcheck",
    "pytest",
    "eslint",
    "go-vet"
  ],
  "quality_gate_retries": 1,
  "max_review_cycles": 1,
  "queue_enabled": true,
  "max_queue_size": 50,
  "heartbeat_timeout_minutes": 15,
  "stale_state_minutes": 120,
  "adaptive_reason_keywords": [
    "security",
    "auth",
    "authentication",
    "authorization",
    "migration",
    "payment",
    "architecture",
    "refactor"
  ],
  "model_learning_enabled": true,
  "log_retention_days": 7,
  "queue_dispatch_batch": 1,
  "mcp_servers": {
    "erp-db": {
      "command": "/bin/bash",
      "args": [
        "-lc",
        "exec npx -y @bytebase/dbhub --dsn \"${DOTMAC_ERP_DB_DSN:?DOTMAC_ERP_DB_DSN must be set in .env.agent-swarm}\""
      ],
      "cwd": "/home/dotmac/projects/dotmac_erp",
      "description": "DotMac ERP database - read-only queries",
      "enabled": true,
      "timeout": 30
    }
  },
  "single_executor_mode": false,
  "single_executor_engine": "claude",
  "codex_only_execution": false,
  "coordinator_engine": "claude",
  "coordinator_model": "sonnet",
  "pm_engine": "claude",
  "pm_model": "sonnet",
  "fleet_enabled": true,
  "fleet_db_path": "/home/dotmac/projects/.seabone-fleet/fleet.db",
  "fleet_lease_ttl_seconds": 1800,
  "coordinator_mode": "deterministic",
  "taskspec_scoring": {
    "weights": {
      "impact": 35,
      "risk_reduction": 25,
      "urgency": 25,
      "effort_inverse": 15
    }
  },
  "queue_policy": {
    "enabled": true,
    "track_capacity": {
      "reactive": 100,
      "feature": 50,
      "quality": 10,
      "coverage": 8,
      "refactor": 7,
      "reliability": 15,
      "integration": 10,
      "journey": 10,
      "maintenance": 5
    }
  },
  "shared_context_max_entries": 300,
  "codex_model": "o3",
  "claude_model": "sonnet",
  "review_model": "opus",
  "engine": "aider",
  "review_models": {
    "multi_review_enabled": false,
    "models": [
      "opus"
    ],
    "consensus_threshold": 1
  },
  "aider_model": "openrouter/minimax/minimax-m2.5",
  "review_engine": "claude",
  "escalation_engine": "claude",
  "escalation_model": "opus",
  "engine_routing": {
    "lint-fix": "aider",
    "deps": "aider",
    "formatting": "aider",
    "changelog": "aider",
    "readme": "aider",
    "docs": "aider",
    "reactive": "claude",
    "security": "claude",
    "feature": "claude",
    "quality": "claude",
    "api": "claude",
    "coverage": "claude"
  },
  "plan_before_execute": true,
  "plan_engine": "openrouter",
  "plan_model": "anthropic/claude-opus-4-6",
  "focus_rotation": [
    "security",
    "quality",
    "api",
    "deps",
    "coverage",
    "migrations",
    "observability",
    "architecture",
    "tasks",
    "docs",
    "frontend",
    "backend",
    "database",
    "optimization"
  ],
  "auto_merge_enabled": true,
  "auto_merge_tracks": [
    "lint-fix",
    "deps",
    "docs",
    "formatting",
    "changelog",
    "readme"
  ],
  "consolidate_prs": true,
  "_comment": "Shared config \u2014 synced to all repo .seabone/config.json by fleet-config-sync.sh",
  "_comment2": "Keys listed in local_overrides are NOT synced (repos keep their own values)"
}